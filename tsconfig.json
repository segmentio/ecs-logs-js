{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "lib": ["es2023"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "incremental": true,
    "outDir": "dist",
    "declaration": true
  }
}
